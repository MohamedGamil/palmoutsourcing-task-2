{
    "openapi": "3.0.0",
    "info": {
        "title": "PalmOutsourcing Task (2) API",
        "description": "Laravel API for tasks (GET /tasks, POST /tasks).",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8081",
            "description": "Local dev"
        }
    ],
    "paths": {
        "/api": {}
    },
    "components": {
        "schemas": {
            "Product": {
                "title": "Product",
                "description": "Product model",
                "required": [
                    "title",
                    "price",
                    "source_store"
                ],
                "properties": {
                    "id": {
                        "description": "Product ID",
                        "type": "integer",
                        "example": 1
                    },
                    "title": {
                        "description": "Product title",
                        "type": "string",
                        "example": "Wireless Headphones"
                    },
                    "description": {
                        "description": "Product description",
                        "type": "string",
                        "example": "High-quality wireless headphones with noise cancellation"
                    },
                    "price": {
                        "description": "Product price",
                        "type": "number",
                        "format": "float",
                        "example": 99.99
                    },
                    "list_price": {
                        "description": "Product list price",
                        "type": "number",
                        "format": "float",
                        "example": 129.99
                    },
                    "rating": {
                        "description": "Product rating (1-5)",
                        "type": "number",
                        "format": "float",
                        "maximum": 5,
                        "minimum": 1,
                        "example": 4.5
                    },
                    "rating_count": {
                        "description": "Number of ratings",
                        "type": "integer",
                        "example": 150
                    },
                    "vendor_name": {
                        "description": "Vendor name",
                        "type": "string",
                        "example": "TechStore Inc."
                    },
                    "image_url": {
                        "description": "Product image URL",
                        "type": "string",
                        "example": "https://example.com/images/product.jpg"
                    },
                    "source_store": {
                        "description": "Source store",
                        "type": "string",
                        "enum": [
                            "amazon",
                            "jumia"
                        ],
                        "example": "amazon"
                    },
                    "store_category": {
                        "description": "Store category",
                        "type": "string",
                        "example": "Electronics"
                    },
                    "store_url": {
                        "description": "Product URL on source store",
                        "type": "string",
                        "example": "https://www.amazon.com/product/123"
                    },
                    "created_at": {
                        "description": "Creation timestamp",
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T12:00:00Z"
                    },
                    "updated_at": {
                        "description": "Last update timestamp",
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T12:00:00Z"
                    }
                },
                "type": "object"
            },
            "User": {
                "title": "User",
                "required": [
                    "name",
                    "email",
                    "password"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": true,
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "John Doe"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "john@example.com"
                    },
                    "email_verified_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-06-01T12:00:00Z",
                        "nullable": true
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "writeOnly": true,
                        "example": "secret"
                    },
                    "remember_token": {
                        "type": "string",
                        "example": "randomtoken123",
                        "nullable": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true,
                        "example": "2024-06-01T12:00:00Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "readOnly": true,
                        "example": "2024-06-01T12:00:00Z"
                    }
                },
                "type": "object"
            }
        }
    }
}